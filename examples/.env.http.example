# Environment file example
# Copy this file to .env.http and add your actual values
# Load this file using: require('nrest').setup({ env_file = '.env.http' })

# API Configuration
@baseUrl = https://api.production.com
@apiVersion = v2

# Authentication (REPLACE WITH YOUR TOKENS!)
@authToken = Bearer your-token-here
@apiKey = sk_live_your-api-key-here
@GITLAB_TOKEN = glpat-your-gitlab-token-here

# Common Headers
@userAgent = nrest.nvim/1.0
@acceptType = application/json
